{
  "manifest_version": 3,
  "name": "動く動物達",
  "version": "1.0",
  "description": "画面上をかわいい動物達が動くChrome拡張機能です。",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content_whale.js", "content_girl.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "images/whale/whale_1.png",
        "images/whale/whale_2.png",
        "images/whale/whale_3.png",
        "images/whale/whale_4.png",
        "images/whale/whale_5.png",
        "images/girl/girl_1.png",
        "images/girl/girl_2.png",
        "images/girl/girl_3.png",
        "images/girl/girl_4.png",
        "images/girl/girl_5.png"
      ],
      "matches": ["<all_urls>"]
    }
  ]
}